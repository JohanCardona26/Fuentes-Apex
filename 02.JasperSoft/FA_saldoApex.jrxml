	<parameter name="NIT_CLIENTE" class="java.lang.String"/>
	<parameter name="NUMERO_OBLIGACION" class="java.lang.String"/>
	<parameter name="TIPO_DOC" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
    O.VKPNUMERO, 
    C.VKPCODIGO, 
    C.VRNOMBRE || ' ' || c.VAPELLIDO1 || ' ' || c.vapellido2 Nombrecliente, 
    --to_char(O.MRSALDO, 'FML999G999G999G999G990D00') SALDO, 
    TO_CHAR(NVL(FUK_GASTOS_COBRANZAS(O.TIPODOC_VKPCODIGO, O.VKPNUMERO, SYSDATE), 0 ) 
    +  NVL(FUK_SALDOOBLIGA_TOTALYH (O.TIPODOC_VKPCODIGO, O.VKPNUMERO),0), 'FML999G999G999G999G990D00') SALDO,
    NVL(dp_comun.leer('EMP_CELAPEX'), '') DESDE_CELULAR, 
    NVL(dp_comun.leer('EMP_TMEDAPEX'), '') MEDELLIN, 
    NVL(dp_comun.leer('EMP_TBOGAPEX'), '') BOGOTA, 
    NVL(dp_comun.leer('EMP_TELCALI'), '') CALI, 
    NVL(dp_comun.leer('EMP_TOTRAPEX'), '') OTRAS_ZONAS, 
    ( SELECT VCORREO FROM TXEMPRESAS WHERE VRNIT = '901603279' ) CORREO, 
   	Replace(to_char(sysdate, 'DD "de "Month" del" YYYY'), '  ', '') fecha, 
    ( SELECT VRNIT FROM TXEMPRESAS WHERE VRNIT = '901603279' ) VRNIT, 
    DECODE(FX_Estatus_Usuario_Apex('USU_TIPOAPEX', UPPER(U.VKPUSUARIOORACLE)), 'D', 'el cliente' , 'R', '') Complemento, 
    DECODE(FX_Estatus_Usuario_Apex('USU_TIPOAPEX', UPPER(U.VKPUSUARIOORACLE)), 'D', 'distribuidor' , 'R', ' ') rol, 
    DECODE(FX_Estatus_Usuario_Apex('USU_TIPOAPEX', UPPER(U.VKPUSUARIOORACLE)), 'D', 'con Nit' , 'R', 'identificado con cédula de ciudadania') tipo_documentoId 
FROM TKOBLIGACIONES O, TXTERCEROS C, TXUSUARIOS U 
WHERE c.vkpcodigo = O.CLIENTE_TERC_VKPCODIGO 
AND O.CLIENTE_TERC_VKPCODIGO = u.EMPLE_PERSO_TERC_VKPCODIGO 
AND O.FRANULADO = 'N' 
and NVL(FUK_GASTOS_COBRANZAS(O.TIPODOC_VKPCODIGO, O.VKPNUMERO, SYSDATE), 0 ) 
  + NVL(FUK_SALDOOBLIGA_TOTALYH (O.TIPODOC_VKPCODIGO, O.VKPNUMERO),0) > 0
AND C.vkpcodigo = $P{NIT_CLIENTE} 
AND o.vkpnumero = $P{NUMERO_OBLIGACION} 
AND O.TIPODOC_VKPCODIGO = $P{TIPO_DOC}]]>
	</queryString>
