	<parameter name="NIT_CLIENTE" class="java.lang.String"/>
	<parameter name="ANIO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT tkobligaciones.VKPNUMERO  numeroCredito,
       txterceros.VKPCODIGO nitCliente,
       txterceros.VRNOMBRE || ' ' || txterceros.vapellido1 || ' ' || txterceros.vapellido2 nombreCliente,
       to_char(tkobligaciones.MRSALDO, 'FML999G999G999G999G990') saldoObligacion,
       Replace(to_char(sysdate, 'DD "de "Month"del" YYYY'), '  ', '') fechaExpedicion,
       $P{ANIO}  anoGravable,
       tkobligaciones.VSUBCATEGORIA DESTINACION,
       TKOBLIGACIONES.DRFECHA fechaObligacion,
       to_char(tkobligaciones.mrcapital, 'FML999G999G999G999G990') valorCredito,
       to_char(NVL(( FUK_OF_VALORES_DOCS (tkobligaciones.VKPNUMERO , tkobligaciones.TIPODOC_VKPCODIGO, 'IMPA', $P{ANIO} )),0),'FML999G999G999G999G990') interesMoraPagado,
       to_char(NVL(( FUK_OF_VALORES_DOCS (tkobligaciones.VKPNUMERO , tkobligaciones.TIPODOC_VKPCODIGO, 'ICPA', $P{ANIO} )),0),'FML999G999G999G999G990') interesCausadoPagado,
       to_char(NVL(( FUK_OF_VALORES_DOCS (tkobligaciones.VKPNUMERO , tkobligaciones.TIPODOC_VKPCODIGO, 'SFA' , $P{ANIO} )),0),'FML999G999G999G999G990') saldoCierreAno,
       to_char(NVL(( FUK_OF_VALORES_DOCS (tkobligaciones.VKPNUMERO , tkobligaciones.TIPODOC_VKPCODIGO, 'SCFA', $P{ANIO} )),0),'FML999G999G999G999G990') saldoCapitalCierre,
       to_char(NVL(( FUK_OF_VALORES_DOCS (tkobligaciones.VKPNUMERO , tkobligaciones.TIPODOC_VKPCODIGO, 'IPA' , $P{ANIO} )),0),'FML999G999G999G999G990') interesPagadoAnual,
       to_char(NVL(( FUK_OF_VALORES_DOCS (tkobligaciones.VKPNUMERO , tkobligaciones.TIPODOC_VKPCODIGO, 'OTR' , $P{ANIO} )),0), 'FML999G999G999G999G990') otrosCargos,
       to_char(NVL(( FUK_OF_VALORES_DOCS (tkobligaciones.VKPNUMERO , tkobligaciones.TIPODOC_VKPCODIGO, 'COBPAGANO' , $P{ANIO} )),0), 'FML999G999G999G999G990') cobranzaPagada,
       to_char(NVL(( FUK_OF_VALORES_DOCS (tkobligaciones.VKPNUMERO , tkobligaciones.TIPODOC_VKPCODIGO, 'SEGPAGANO' , $P{ANIO} )),0), 'FML999G999G999G999G990') segurosPagos,
       to_char(NVL(( FUK_OF_VALORES_DOCS (tkobligaciones.VKPNUMERO , tkobligaciones.TIPODOC_VKPCODIGO, 'CAPITANUAL' , $P{ANIO} )),0), 'FML999G999G999G999G990') captalPago,
       to_char(NVL(( FUK_OF_VALORES_DOCS (tkobligaciones.VKPNUMERO , tkobligaciones.TIPODOC_VKPCODIGO, 'OTRVALOR' , $P{ANIO} )),0), 'FML999G999G999G999G990') OTROSVALORES,
       to_char(NVL(( FUK_OF_VALORES_DOCS (tkobligaciones.VKPNUMERO , tkobligaciones.TIPODOC_VKPCODIGO, 'FCC' , $P{ANIO} )),0), 'FML999G999G999G999G990') FONDOCOBERTURAS,
       case 
            when Tipcliente_Vkpcodigo = dp_comun.leer('EMP_TIPCLIEW') then
                'Que ' || txterceros.VRNOMBRE || ' ' || txterceros.vapellido1 || ' ' || txterceros.vapellido2 ||', identificado/a con Nit '|| txterceros.vkpcodigo || ', durante el año '|| $P{ANIO} ||' presentó la siguiente información:'
            else
                'Que el/la señor/a ' || txterceros.VRNOMBRE || ' ' || txterceros.vapellido1 || ' ' || txterceros.vapellido2 ||', identificado/a con cedula de ciudadanía número '|| txterceros.vkpcodigo || ', durante el año '|| $P{ANIO} ||'  presentó la siguiente información:'
       end as Leyenda,
       (
            SELECT VRNIT || '-' || NDIGITOVERIFICACION
            FROM TXEMPRESAS
            WHERE MOD_VKPCODIGO = 'YAMAHA'
       ) nitEmpresa
FROM TKOBLIGACIONES, 
     TXTERCEROS,
     TKCLIENTES
WHERE TKOBLIGACIONES.CLIENTE_TERC_VKPCODIGO     = TXTERCEROS.vkpcodigo
  AND TXTERCEROS.vkpcodigo                      = TKCLIENTES.TERC_VKPCODIGO
  AND TXTERCEROS.VKPCODIGO                      = $P{NIT_CLIENTE}
  and to_char(TKOBLIGACIONES.DRFECHA, 'YYYY')          <= $P{ANIO}
  AND to_char(TKOBLIGACIONES.DVENCIMIENTO, 'YYYY')     >= $P{ANIO}
  AND NVL(TKOBLIGACIONES.FRANULADO,'N')           = 'N'
  and TKOBLIGACIONES.TIPODOC_VKPCODIGO            != '01SFR'
  and TKOBLIGACIONES.TIPODOC_VKPCODIGO            != '01SFW'
ORDER BY TKOBLIGACIONES.DRFECHA DESC]]>
	</queryString>
